<+><?xml version=\"1.0\" encoding=\"UTF-8\"?>\n<!-- 日志级别从低到高分为TRACE < DEBUG < INFO < WARN < ERROR < FATAL，如果设置为WARN，则低于WARN的信息都不会输出 -->\n<configuration scan=\"true\" scanPeriod=\"10 seconds\">\n\n    <contextName>logback</contextName>\n    <!-- name的值是变量的名称，value的值时变量定义的值。通过定义的值会被插入到logger上下文中。定义变量后，可以使“${}”来使用变量。 -->\n    <springProperty scope=\"context\" name=\"log.path\" source=\"logging.path\"/>\n    <!-- 日志格式 -->\n    <conversionRule conversionWord=\"clr\" converterClass=\"org.springframework.boot.logging.logback.ColorConverter\"/>\n    <conversionRule conversionWord=\"wex\"\n                    converterClass=\"org.springframework.boot.logging.logback.WhitespaceThrowableProxyConverter\"/>\n    <conversionRule conversionWord=\"wEx\"\n                    converterClass=\"org.springframework.boot.logging.logback.ExtendedWhitespaceThrowableProxyConverter\"/>\n\n    <!-- 输出到控制台 -->\n    <appender name=\"CONSOLE\" class=\"ch.qos.logback.core.ConsoleAppender\">\n        <!-- 此日志appender是为开发使用，只配置最底级别，控制台输出的日志级别是大于或等于此级别的日志信息 -->\n        <filter class=\"ch.qos.logback.classic.filter.ThresholdFilter\">\n            <level>info</level>\n        </filter>\n        <encoder>\n            <pattern>%d{yy-MM-dd.HH:mm:ss.SSS} [%-16t] %-5p %-22c{0}%X{ServiceId} -%X{trace-id} %m%n</pattern>\n            <charset>UTF-8</charset>\n        </encoder>\n    </appender>\n\n    <!--输出到文件-->\n    <!-- 时间滚动输出 level为 INFO 日志 -->\n    <appender name=\"INFO_FILE\" class=\"ch.qos.logback.core.rolling.RollingFileAppender\">\n        <!-- 正在记录的日志文件的路径及文件名 -->\n        <file>./data/log/log_info.log</file>\n        <!--日志文件输出格式-->\n        <encoder>\n            <pattern>%d{yy-MM-dd.HH:mm:ss.SSS} [%-16t] %-5p %-22c{0}%X{ServiceId} -%X{trace-id} %m%n</pattern>\n            <charset>UTF-8</charset>\n        </encoder>\n        <!-- 日志记录器的滚动策略，按日期，按大小记录 -->\n        <rollingPolicy class=\"ch.qos.logback.core.rolling.TimeBasedRollingPolicy\">\n            <!-- 每天日志归档路径以及格式 -->\n            <fileNamePattern>./data/log/log-info-%d{yyyy-MM-dd}.%i.log</fileNamePattern>\n            <timeBasedFileNamingAndTriggeringPolicy class=\"ch.qos.logback.core.rolling.SizeAndTimeBasedFNATP\">\n                <maxFileSize>100MB</maxFileSize>\n            </timeBasedFileNamingAndTriggeringPolicy>\n            <!--日志文件保留天数-->\n            <maxHistory>15</maxHistory>\n            <totalSizeCap>10GB</totalSizeCap>\n        </rollingPolicy>\n    </appender>\n\n    <!-- 时间滚动输出 level为 ERROR 日志 -->\n    <appender name=\"ERROR_FILE\" class=\"ch.qos.logback.core.rolling.RollingFileAppender\">\n        <!-- 正在记录的日志文件的路径及文件名 -->\n        <file>./data/log/log_error.log</file>\n        <!--日志文件输出格式-->\n        <encoder>\n            <pattern>%d{yy-MM-dd.HH:mm:ss.SSS} [%-16t] %-5p %-22c{0}%X{ServiceId} -%X{trace-id} %m%n</pattern>\n            <charset>UTF-8</charset>\n        </encoder>\n        <!-- 日志记录器的滚动策略，按日期，按大小记录 -->\n        <rollingPolicy class=\"ch.qos.logback.core.rolling.TimeBasedRollingPolicy\">\n            <fileNamePattern>./data/log/log-error-%d{yyyy-MM-dd}.%i.log</fileNamePattern>\n            <timeBasedFileNamingAndTriggeringPolicy class=\"ch.qos.logback.core.rolling.SizeAndTimeBasedFNATP\">\n                <maxFileSize>100MB</maxFileSize>\n            </timeBasedFileNamingAndTriggeringPolicy>\n            <!-- 日志文件保留天数【根据服务器预留，可自行调整】 -->\n            <maxHistory>7</maxHistory>\n            <totalSizeCap>5GB</totalSizeCap>\n        </rollingPolicy>\n        <!-- WARN 级别及以上 -->\n        <filter class=\"ch.qos.logback.classic.filter.ThresholdFilter\">\n            <level>WARN</level>\n        </filter>\n    </appender>\n\n    <!-- 异步输出 -->\n    <appender name=\"ASYNC_FILE_INFO\" class=\"ch.qos.logback.classic.AsyncAppender\">\n        <!-- 队列剩余容量小于discardingThreshold,则会丢弃TRACT、DEBUG、INFO级别的日志;默认值-1,为queueSize的20%;0不丢失日志 -->\n        <discardingThreshold>0</discardingThreshold>\n        <!-- 更改默认的队列的深度,该值会影响性能.默认值为256 -->\n        <queueSize>8192</queueSize>\n        <!-- neverBlock:true 会丢失日志,但业务性能不受影响 -->\n        <neverBlock>true</neverBlock>\n        <!--是否提取调用者数据-->\n        <includeCallerData>false</includeCallerData>\n        <appender-ref ref=\"INFO_FILE\"/>\n    </appender>\n\n    <appender name=\"ASYNC_FILE_ERROR\" class=\"ch.qos.logback.classic.AsyncAppender\">\n        <!-- 队列剩余容量小于discardingThreshold,则会丢弃TRACT、DEBUG、INFO级别的日志;默认值-1,为queueSize的20%;0不丢失日志 -->\n        <discardingThreshold>0</discardingThreshold>\n        <!-- 更改默认的队列的深度,该值会影响性能.默认值为256 -->\n        <queueSize>1024</queueSize>\n        <!-- neverBlock:true 会丢失日志,但业务性能不受影响 -->\n        <neverBlock>true</neverBlock>\n        <!--是否提取调用者数据-->\n        <includeCallerData>false</includeCallerData>\n        <appender-ref ref=\"ERROR_FILE\"/>\n    </appender>\n\n    <!-- 开发环境：控制台打印 -->\n    <springProfile name=\"dev\">\n        <logger name=\"com.nmys.view\" level=\"debug\"/>\n    </springProfile>\n\n<!--    <appender name=\"CUSTOM\" class=\"plus.gaga.monitor.sdk.appender.CustomAppender\">-->\n<!--        <systemName>big-market</systemName>-->\n<!--        <groupId>cn.bugstack</groupId>-->\n<!--        <host>127.0.0.1</host>-->\n<!--        <port>16379</port>-->\n<!--    </appender>-->\n\n    <root level=\"info\">\n<!--        <appender-ref ref=\"CUSTOM\"/>-->\n\n        <appender-ref ref=\"CONSOLE\"/>\n        <!-- 异步日志-INFO -->\n        <appender-ref ref=\"ASYNC_FILE_INFO\"/>\n        <!-- 异步日志-ERROR -->\n        <appender-ref ref=\"ASYNC_FILE_ERROR\"/>\n    </root>\n\n</configuration>\n
